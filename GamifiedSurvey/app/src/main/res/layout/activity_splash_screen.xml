<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screen"
    tools:context=".SplashScreen">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="176dp"
        android:layout_height="183dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="42dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="506dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logofast" />

    <TextView
        android:id="@+id/uniName"
        style="bold"
        android:layout_width="match_parent"
        android:layout_height="122dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="265dp"
        android:layout_marginEnd="-9dp"
        android:layout_marginBottom="378dp"
        android:text="@string/elevated_quantity_and_quality_of_ema_responses_via_gamification"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.879" />

    <TextView
        android:id="@+id/splashdes"
        android:layout_width="366dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="396dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="259dp"
        android:background="@color/white"
        android:text="@string/theses"
        android:textAlignment="center"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView"
        android:layout_width="175dp"
        android:layout_height="158dp"
        android:layout_marginStart="119dp"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="43dp"
        android:src="@drawable/avt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></androidx.appcompat.widget.AppCompatImageView>

</androidx.constraintlayout.widget.ConstraintLayout>